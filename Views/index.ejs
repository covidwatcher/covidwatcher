<!DOCTYPE html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>COVID Watcher</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="./CSS/styles.css">
    </head>
    <body>
        <header>
            <h1>COVID Watcher</h1>
            <ul>
                <li><a href='./index.ejs'>Home</a></li>
                <li><a href='./state.ejs'>State Stats</a></li>
                <li><a href='./articles.ejs'>Articles</a></li>
                <li><a href='./about.ejs'>About</a></li>
            </ul>
        </header>
        <main>
            <div id="state-container">
                <% arrayOfStateData.forEach(state => { %>
                <div class="state-card">
                  <h2><%= state.stateName %></h2>
                  <p><%= state.positive %></p>
                  <p><%= state.negative %></p>
                  <p><%= state.hospitalizedCurrently %></p>
                  <p><%= state.recovered %></p>
                  <p><%= state.death %></p>
                  <p><%= state.totalTest %></p>
                  <p><%= state.positiveTests %></p>
                  <p><%= state.negativeTests %></p>
                  <button type="button" post%=>>View Details</button>
                  <% }) %>
                </div>
              </div>
        </main>
        
        
        <script src="" async defer></script>
    </body>
</html>